generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Todos {
  id        Int      @id @default(autoincrement())
  title     String   @default("")
  action    String   @default("TODO")
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
  deletedAt DateTime

  @@map("todos")
}
