generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model todo_indicator {
  id        Int     @id @default(autoincrement())
  indicator String? @db.VarChar(255)
  todos     todos[]
}

model todos {
  id             Int            @id @default(autoincrement())
  title          String         @default("")
  indicator_id   Int            @default(1)
  createdAt      DateTime       @default(now())
  updatedAt      DateTime       @default(now())
  deletedAt      DateTime?
  todo_indicator todo_indicator @relation(fields: [indicator_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "add_foreign_key")

  @@index([indicator_id], map: "add_foreign_key")
}
